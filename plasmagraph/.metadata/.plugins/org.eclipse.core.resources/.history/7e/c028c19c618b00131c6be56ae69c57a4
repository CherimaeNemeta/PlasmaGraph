public class ReferenceTests {
    
    @Test
    public void testAdd () {
        DataReference r1 = new DataReference ();
        assertTrue (r1.add (new Pair (1, 2, "Dummy Reference")));
        assertFalse (r1.add (new Pair (-1, Integer.MIN_VALUE, "Dummy Reference 2")));
    }
    
