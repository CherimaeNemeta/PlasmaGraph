package org.pvg.plasmagraph.tests;

import static org.junit.Assert.*;

import org.junit.Test;
import org.pvg.plasmagraph.utils.graphs.ChartType;
import org.pvg.plasmagraph.utils.template.InterpolationType;
import org.pvg.plasmagraph.utils.template.OutlierResponse;
import org.pvg.plasmagraph.utils.template.Template;

public class TemplateTests {
    
    @Test
    public void testSaveAndOpenTemplate () {
        Template t1 = new Template ();
        t1.setChartName ("Name");
        t1.setChartType (ChartType.XY_GRAPH);
        t1.setInterpolationType (InterpolationType.POWER);
        t1.setOutlierResponse (OutlierResponse.REMOVE);
        
        t1.saveTemplate (new JFileChooser ());
    }
    
}
